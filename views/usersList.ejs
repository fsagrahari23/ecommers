<%- include('./partials/OwnHea') %>


<div class="md-conatiner mt-10">
<h2 class="text-xl font-bold mb-4 text-center">All Users</h2>
<table class="min-w-full bg-white rounded shadow-md">
  <thead>
    <tr>
      <th class="py-2 px-4 border-b">Name</th>
      <th class="py-2 px-4 border-b">Email</th>
      <th class="py-2 px-4 border-b">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% owner.users.forEach(user => { %>
    <tr>
      <td class="py-2 px-4 border-b text-center"><%= user.fullname %></td>
      <td class="py-2 px-4 border-b text-center"><%= user.email %></td>
      <td class="py-2 px-4 border-b text-center">
        <a href="/owner/user/<%= user._id %>" class="text-blue-600 hover:underline ">View Profile</a>/<a href="/owner/user/<%= user._id %>/delete" class="text-red-600 hover:underline ">Delete User</a>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
</div>








<%- include('./partials/footer') %>
